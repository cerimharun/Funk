<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>0.0.5</Version>
    <Authors>Harun Ćerim</Authors>
    <Company />
    <Description>Minimalistic Functional Library for C#.</Description>
    <PackageLicenseFile>license.txt</PackageLicenseFile>
    <PackageProjectUrl>https://github.com/hcerim/Funk</PackageProjectUrl>
    <PackageIcon>vinyl.png</PackageIcon>
    <RepositoryUrl>https://github.com/hcerim/Funk</RepositoryUrl>
    <PackageTags>Funk, Functional C#, Immutable Types, Pattern Matching, Monads, Functors, Railway Programming, Async, Higher-Order Functions</PackageTags>
    <AssemblyVersion>0.0.5.0</AssemblyVersion>
    <FileVersion>0.0.5.0</FileVersion>
    <Copyright>Copyright © 2020</Copyright>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes>MapFailure methods added for Exc monad (for details look at tests or Demo project).
Cleanup and performance improvements.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Collections.Immutable" Version="1.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\Files\license.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="..\Files\vinyl.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
